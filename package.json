{
  "name": "whoscored",
  "version": "0.0.1",
  "description": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "jest __tests__ && tslint src/**/*.ts",
    "lint": "tslint src/**/*.ts",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/(?:.+?)/__tests__/"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    }
  },
  "author": "dme",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^11.13.0",
    "coveralls": "^3.0.3",
    "jest": "^24.0.11",
    "tslint": "^5.15.0"
  },
  "dependencies": {
    "axios": "0.19.2",
    "jsdom": "^16.5.0",
    "typescript": "^3.4.1"
  }
}
